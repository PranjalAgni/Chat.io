{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "mocha": true,
        "browser": true
    },
    "rules": {
        "no-console": 0,
        "indent": ["error",4],
        "semi": [2, "always"],
        "curly":"error"
    },
    "globals": {
        "io": true,
        "jQuery": true
    }
}